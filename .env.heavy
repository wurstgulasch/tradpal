# Heavy Konfiguration - Volle Funktionalität
# Alle Features aktiviert für maximale Performance

# ML-Features vollständig aktivieren
ML_ENABLED=true
ML_USE_PYTORCH=true
ML_USE_AUTOML=true
ML_USE_ENSEMBLE=true

# Adaptive Optimization aktivieren
ADAPTIVE_OPTIMIZATION_ENABLED=true

# Multi-Timeframe Analysis aktivieren
MTA_ENABLED=true

# Monitoring Stack aktivieren
MONITORING_STACK_ENABLED=true
PROMETHEUS_ENABLED=true
PERFORMANCE_MONITORING_ENABLED=true

# WebSocket Features aktivieren
WEBSOCKET_ENABLED=true
REALTIME_ENABLED=true
WEBSOCKET_DATA_ENABLED=true

# Parallel Processing vollständig aktivieren
PARALLEL_PROCESSING_ENABLED=true
PARALLEL_BACKTESTING_ENABLED=true

# Redis aktivieren (falls verfügbar)
REDIS_ENABLED=true

# Rate Limiting aktivieren
ADAPTIVE_RATE_LIMITING_ENABLED=true

# Basis-Konfiguration
SYMBOL=BTC/USDT
TIMEFRAME=1m
CONFIG_MODE=discovery