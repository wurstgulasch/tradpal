# Ressourcenschonende Konfiguration für MacBook Air
# Temporäre Einstellungen für Live-Testing ohne Überlastung

# ML-Features abschalten (sehr ressourcenintensiv)
ML_ENABLED=false
ML_USE_PYTORCH=false
ML_USE_AUTOML=false
ML_USE_ENSEMBLE=false

# Adaptive Optimization abschalten (genetische Algorithmen)
ADAPTIVE_OPTIMIZATION_ENABLED=false
ADAPTIVE_CONFIG_FILE=config/adaptive_config_light.json

# Multi-Timeframe Analysis abschalten
MTA_ENABLED=false

# Monitoring Stack abschalten
MONITORING_STACK_ENABLED=false
PROMETHEUS_ENABLED=false
PERFORMANCE_MONITORING_ENABLED=false

# WebSocket Features abschalten
WEBSOCKET_ENABLED=false
REALTIME_ENABLED=false
WEBSOCKET_DATA_ENABLED=false

# Parallel Processing abschalten (kann Ressourcen verbrauchen)
PARALLEL_PROCESSING_ENABLED=false
PARALLEL_BACKTESTING_ENABLED=false

# Redis abschalten (falls nicht verfügbar)
REDIS_ENABLED=false

# Rate Limiting beibehalten (nicht ressourcenintensiv)
ADAPTIVE_RATE_LIMITING_ENABLED=true

# Basis-Konfiguration
SYMBOL=BTC/USDT
TIMEFRAME=1m
CONFIG_MODE=conservative